    <form class="container-sm pt-3 p-5 border bg-white shadow w-75">

        <div class="my-3 text-start">
          

            <button type="button" id="sair" class="btn btn-primary ms-5" data-bs-toggle="modal" data-bs-target="#JanelaModalSair">Novo Cadastro</button>

            <div id="JanelaModalSair" class="modal fade " data-bs-backdrop="static">

                <div class="modal-dialog modal-lg modal-dialog-centered ">

                    <div class="modal-content">
 
                        <div class="modal-header align-self-center">
                            <h2 class="modal-title">Cadastra Novo Paciente</h2>
                        </div>

                        <div class="modal-body">
                            <p><h3 class="text-center">CONFIRMAÇÃO</h3></p>
                            <p class="text-center">Para realizar um novo cadastro de paciente - Confirmar.</p>
                            <p class="text-center">Para cancelar o novo de cadastro de paciente - Fechar.</p>
                        </div>

                        <div class="modal-footer ">
                            
                            <a href="/ficha.html" class="btn btn-primary">Confirmar</a>
                            <button type="button" class="btn btn-danger" data-bs-toggle="modal">Fechar</button>
                            
                        </div>
                    </div>
                </div>
            </div>

            <button type="button" id="atualizar" class="btn btn-primary ms-5" data-bs-toggle="modal" data-bs-target="#JanelaModalAtualizar">Adicionar Exame</button>

            <div id="JanelaModalAtualizar" class="modal fade " data-bs-backdrop="static">

                <div class="modal-dialog modal-lg modal-dialog-centered ">

                    <div class="modal-content">
 
                        <div class="modal-header align-self-center">
                            <h2 class="modal-title">Novo Exame</h2>
                        </div>

                        <div class="modal-body">
                            <p><h4 class="text-center">PARABÉNS <br>Um novo exame foi incluído com sucesso!</h4></p>
                            <p class="text-center">Para imprimir a ficha do paciente - Imprimir.</p>
                            <p class="text-center">Para canlelar - Fechar.</p>
                        </div>

                        <div class="modal-footer ">
                            <button type="button" class="btn btn-primary" >Imprimir</button>
                            <button type="button" class="btn btn-danger" data-bs-toggle="modal">Fechar</button>
                        </div>

                    </div>
                </div>
            </div>

            <button onclick="imprimirFicha()" class="btn btn-primary ms-5">Imprimir</button>

        </div>

        <div class="container-fluid border border-1 border-drak mb-5 p-4 bg-white shadow">

                <% if (search) { %>
                  <p>Você pesquisou por: <%= search %></p>
                  <% if (records.length === 0) { %>
                    <p>Nenhum registro encontrado para '<%= search %>'.</p>
                  <% } else { %>
                    <ul>
                      <% records.forEach(function(record) { %>
                        <div class="my-3 col-2">
                            <label class="form-label">Registro Cliente:</label>
                            <input id="id_paciente" class="form-control" type="text" name="id_paciente" value="<%= record.id_paciente %>">
                        </div>
                        <div class="row">
                            <div class="my-3 col-5">
                                <label class="form-label">Nome:</label>
                                <input id="nome" class="form-control" type="text" name="nome" value="<%= record.nome %>">
                            </div>
                            
                            <div class="my-3 col">
                                <label class="form-label">CPF:</label>
                                <input id="cpf" class="form-control" type="number" name="cpf" value="<%= record.cpf %>">
                            </div>
                            
                            <div class="my-3 col">
                                <label class="form-label">RG:</label>
                                <input id="rg" class="form-control" type="number" name="rg" value="<%= record.rg %>">
                            </div>
                        </div>
                          
                        <div class="row">

                            <div class="my-3 col">
                                <label class="form-label">Email:</label>
                                <input id="email" class="form-control" type="text" name="email" value="<%= record.email %>">
                            </div>
            
                            <div class="my-3 col">
                                <label class="form-label">Data de Nascimento:</label>
                                <input id="data_nascimento" class="form-control" type="date" name="data_nascimento" value="<%= record.data_nascimento %>">
                            </div>
            
                            <div class="my-3 col">
                                <label class="form-label">Telefone:</label>
                                <input id="telefone" class="form-control" type="number" name="telefone" value="<%= record.telefone %>">
                            </div>
            
                            <div class="my-3 col">
                                <label class="form-label">Celular:</label>
                                <input id="celular" class="form-control" type="number" name="celular" value="<%= record.celular %>">
                            </div>
            
                        </div>

                        <div class="row ">

                            <div class="my-3 col-2 ms-4 ">
                                <label class="form-label "><h5>Sexo:</h5></label>  <br>
                                <input type="radio" id="feminino" name="sexo"  value="feminino" value="<%= record.feminino %>"> 
                                <label for="feminino" class="ms-2"><h6>Feminino</h6></label> <br>
                                <input type="radio" id="masculino" name="sexo" id="masculino" value="masculino" value="<%= record.masculino %>">
                                <label for="masculino" class="ms-2"><h6>Masculino</h6></label>
                            </div>
            
                            <div class="my-3 col-2" col>
                                <label for="peso" class="form-label">Peso:</label>
                                <input id="peso" class="form-control" type="number" name="peso" value="<%= record.peso %>">
                            </div>
            
                            <div class="my-3 col-2 ms-5">
                                <label class="form-label">Altura:</label>
                                <input id="altura" class="form-control" type="number" name="altura" value="<%= record.altura %>">
                            </div>
            
                        </div>
                
        </div>                
        <div class="container-fluid border border-1 border-drak mb-5 p-4 bg-white shadow">
            <div class="row">
                    
                <div class="my-3 col-5">
                    <label class="form-label">Endereço:</label>
                    <input id="endereco" class="form-control" type="text" name="endereco"  value="<%= record.endereco %>">
                </div>

                <div class="my-3 col-2">
                    <label class="form-label">Nº:</label>
                    <input id="numero" class="form-control" type="number" name="numero" value="<%= record.numero %>">
                </div>

                <div class="my-3 col">
                    <label class="form-label">Complemento:</label>
                    <input id="complemento" class="form-control" type="text" name="complemento" value="<%= record.complemento %>">
                </div>
                
            </div>

            <div class="row">

                <div class="my-3 col">
                    <label class="form-label">Bairro:</label>
                    <input id="bairro" class="form-control" type="text" name="bairro" value="<%= record.bairro %>">
                </div>

                <div class="my-3 col">
                    <label class="form-label">Cidade:</label>
                    <input id="cidade" class="form-control" type="text" name="cidade" value="<%= record.cidade %>">
                </div>

                <div class="my-3 col">
                    <label class="form-label">Estado:</label>
                    <input id="estado" class="form-control" type="text" name="estado" value="<%= record.estado %>">
                </div>

                <div class="my-3 col">
                    <label class="form-label">CEP:</label>
                    <input id="cep" class="form-control" type="number" name="cep" value="<%= record.cep %>">
                </div>

            </div>
        
        </div>   
        
        <div class=" container-fluid border bg-white shadow">

            <div class="row">

                <div class="my-3 col-3">
                    <label class="form-label">Nome do Exame:</label>
                    <input id="nome_exame" class="form-control" type="text" name="nome_exame" value="<%= record.nome_exame %>">
                </div>

                <div class="my-3 col-3">
                    <label class="form-label">Tipo de Exame:</label>
                    <input id="tipo_exame" class="form-control" type="text" name="tipo_exame" value="<%= record.tipo_exame %>">
                </div>

                <div class="my-3 col">
                    <label class="form-label">Convênio:</label>
                    <input id="convenio" class="form-control" type="text" name="convenio" value="<%= record.convenio %>">
                </div>

                <div class="my-3 col">
                    <label class="form-label">Data do Exame:</label>
                    <input id="data_exame" class="form-control" type="date" name="data_exame" value="<%= record.data_exame %>">
                </div>
                
            </div>

            <div class="row">

                

                <div class="my-3 col-3">
                    <label class="form-label">Médico:</label>
                    <input id="medico" class="form-control" type="text" name="medico" value="<%= record.medico %>">
                </div>

                <div class="my-3 col-6">
                    <label class="form-label">Comentários:</label>
                    <input id="comentario" class="form-control" type="text" name="comentario" value="<%= record.comentario %>">
                </div>

                <div class="my-3 col">
                    <label class="form-label">Data de entrega:</label>
                    <input id="data_entrega" class="form-control" type="date" name="data_entrega" value="<%= record.data_entrega %>">
                </div>

            </div>
        </div>
                        
                      <% }); %>
                    </ul>
                  <% } %>
                <% } %>
                
        </div>

    </form>
</div>
    <div class="print"> 
        <div class="container-fluid border border-1 border-drak mb-5 p-4 bg-white shadow">
    
            <img src="./imagens/medimagem2.png" alt=""> <br>
    
            <div class="row">
    
